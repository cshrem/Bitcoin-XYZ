include .deosrc


all:
# =========================================================================== #
#	Description: The default rule.
#	Usage: `make` and/or `make all`
# =========================================================================== #
	@-cd ext/bitcoin-core && gitinspector --format html > ../../var/html/bitcoin-core.html
# =========================================================================== #

build:
# =========================================================================== #
#	Description: The build rule.
#	Usage: `make build`
# =========================================================================== #
	@echo $(call l,${cBl},${cGr}[${cPu}5${cGr}] ${cCy}$@)
# =========================================================================== #
